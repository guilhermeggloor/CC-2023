# 1 - Escreva um programa em MIPS que leia um valor a > 0 e imprima
# a soma da sequencia 
# 1, 2, ..., a

 
   
   addi $s1, $zero, 5
   addi $s2, $zero, 0
   
   addi $s1,$s1,1
   addi $s0, $zero, 0
FOR: slt $t0, $s0, $s1
   beq $t0, $zero, FIM_FOR   
   add $s2, $s0 $s2
   addi $s0,$s0,1
   J FOR
   
FIM_FOR:
   
    